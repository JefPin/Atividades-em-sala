#include <stdio.h>
#include <locale.h>
#define SIZE 2

int main(){

	setlocale(LC_ALL,"portuguese");
		
	int i, numerosPares = 0, numerosPositivos = 0, valores[SIZE];
	
	for(i=0;i<SIZE;i++){
		printf("Digite o %dº Valor: ",i+1);
		scanf("%i",&valores[SIZE]);	
}
	
	
	printf("\n=== Exibindo Resultado ===\n");
	for(i=0;i<SIZE;i++){	
	printf("%dº Valor Invertido:  %d \n",i+1); 
}

 	 
	return 0;
}
