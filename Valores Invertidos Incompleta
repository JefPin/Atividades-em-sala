#include <stdio.h>
#include <locale.h>
#define SIZE 2

int main(){

	setlocale(LC_ALL,"portuguese");
		
	int i, numerosPares = 0, numerosPositivos = 0, valores[SIZE];
	
	for(i=0;i<SIZE;i++){
		printf("Digite o %dº Valor: ",i+1);
		scanf("%i",&valores[i]);	
}
		
	printf("\n=== Exibindo Resultado ===\n");
	
	for(i=SIZE-1;i>=0;i--){	
	printf("%dº Valor Invertido:  %d \n",i+1,valores[i]); 
}
 	
	return 0;
}
